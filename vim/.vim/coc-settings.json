{
  "suggest.autoTrigger": "none",
  "coc.preferences.formatOnSaveFiletypes": ["elm","css","js","javascript","rust"],
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    }
  },
  "rust-analyzer.diagnostics.disabled": ["E0601"]
}
